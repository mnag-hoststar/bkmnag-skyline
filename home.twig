{% extends "layout.twig" %}
{% block content %}

<div id="wrap">
	<div class="bk-pt-home">
		<header class="bk-header">
			{{widget('logo', 'logo', {"defaultlogo": asset('images/logo.png'),"showTplWidget":"0"})|raw}}
			{{widget('companyname', 'companyname', {"business": "profile"})|raw}}
		</header>
		<nav class="bk-nav" id="nav">
			{{widget('extendednavigation', 'navigationheader', {"align":"left"})|raw}}
			{{widget('profile', 'footerprofileleft', { 'profileType' : 'socialicons' })|raw}}
		</nav>
		<div role="main" id="main">
			{{widget('feature', 'featurehome', {"header": "profile", "strapline":"profile", "buttonText":"Lorem Ipsum","defaultBgImg": asset('images/feature-bg.jpg'), 'positionTopPercentage': '50','showBtn': true, 'align':'justifycenter'})|raw}}
			<div class="container" id="container">
				{{zone('main')|raw}}
			</div><!--end contain-->
			{% include 'includes/footer.twig' %}
		</div><!--end main-->
	</div><!--end bk-pt-home-->
</div><!--end wrap-->

{% endblock %}